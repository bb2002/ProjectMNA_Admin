<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.saintdev.pmnadmin">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_admin"
        android:label="@string/app_name"
        android:name=".models.datas.profile.kakao.GlobalApplication"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.activitys.LoadingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".views.activitys.MainActivity"
            android:enabled="true"
            android:screenOrientation="portrait" />

        <activity android:name=".views.activitys.WorkspaceActivity"
            android:enabled="true"
            android:screenOrientation="portrait" />

        <activity android:name=".views.activitys.alarms.JoinGrantActivity"
            android:enabled="true"
            android:screenOrientation="portrait" />

        <meta-data android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key" />
    </application>

</manifest>